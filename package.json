{
  "name": "nativescript-masked-text-field",
  "version": "4.0.3",
  "description": "A NativeScript masked text field widget.",
  "main": "masked-text-field",
  "typings": "masked-text-field.d.ts",
  "nativescript": {
    "platforms": {
      "android": "7.0.0",
      "ios": "7.0.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Interface"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-masked-text-field"
  },
  "scripts": {
    "demo-ios": "grunt build && cd demo-vue && tns run ios",
    "demo-android": "grunt build && cd demo-vue && tns run android",
    "demo-ng-ios": "grunt build && cd demo-ng && tns run ios",
    "demo-ng-android": "grunt build && cd demo-ng && tns run android"
  },
  "keywords": [
    "NativeScript",
    "masked input",
    "mask",
    "masked text field",
    "phone",
    "ssn",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "newapx (https://github.com/newapx)"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^15.0.2",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "rxjs": "~5.5.0",
    "tslint": "~5.1.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@nativescript/core": "^8.0.5",
    "@nativescript/types": "^8.0.1"
  }
}
